.header{
  width: 100%;
  height: 40px;
  background:  #1890ff;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.date {
  height: 30px;
  line-height: 30px;
  margin-left: 5%;
  border: 1px solid #000;
  border-radius: 10px;
  padding: 0 5px;
}
.sum {
  margin-right: 5%;
}
.account-container {
  width: 100%;
}
.day-container{
  background: #fff;
  width: 90%;
  margin: 0 auto;
  margin-top: 10px;
  border-radius: 10px;
}

.top {
  width: 100%;
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #EDEDED;
  border-radius: 10px 10px 0px 0px;
}
.left {
  display: flex;
  margin-left: 10px;
}
.weekend {
  margin-left: 3px;
}
.right {
  display: flex;
  margin-right: 10px;
}
/* .output {
  margin-right: 10px;
} */
.account-item {
  width: 100%;
  height: 50px;
  display: flex;
}
.img-container {
  width: 50px;
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.class-img {
  width: 40px;
  height: 40px;
  background: blanchedalmond;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.consumer-details {
  margin-left: 10px;
  margin-right: 10px;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  position: relative;
}
.consumer-details[data-border="true"]{
  border-bottom: 1px solid  #EDEDED;
}
.account-type {
  font-size: medium;
}
.time-masg {
  display: flex;
  font-size: small;
  color:#d0d0d0;
}
.account-divide {
  background: #EDEDED;
  width: 2px;
  height: 10px;
  display: flex;
  align-self: center;
  margin-left: 5px;
  margin-right: 5px;
}
.detail-right {
  position: absolute;
  right: 0;
}